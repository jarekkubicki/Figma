{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",  // Upewnij się, że używasz modułu ES
    "lib": ["es2017", "dom"],
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@figma/plugin-typings"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
